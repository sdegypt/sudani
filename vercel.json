{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "amlhabrak.online"
        }
      ],
      "destination": "https://www.amlhabrak.online/$1",
      "permanent": true
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
